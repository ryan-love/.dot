        <hr>
        <br>


        <form action="/login" method="post" id="login-form">
          <p>Email</p>
          <input type="text" class="form-control col-xs-4" name="email" id="login-form-user"/>
          <br>
          <p>Password</p>
          <input type="password" class="form-control col-xs-4" name="password" id="login-form-pass">
          <br>
          <button type="submit" class="btn btn-primary" id="login-form-button">Submit</button>
        </form>
      </div>
    </div>
    <br>
    <div class="form-group col-md-6">
      <h2>Register</h2>
      <form action="/login/register" method="post">

        <label for="name">First Name</label>
        <input name="firstName" class="form-control col-xs-4" id="fName" placeholder="John">
        <br>
        <label for="name">Last Name</label>
        <input name="lastName" class="form-control col-xs-4" id="lName" placeholder="Doe">
        <br>
        <label for="email">Email</label>
        <input name="email" class="form-control col-xs-4" id="email" placeholder="john@doe.com">
        <br>
        <label for="password">Password</label>
        <input name="password" class="form-control col-xs-4" id="password" placeholder="Secret123" type="password">

        <br>
        <button type="submit" class="btn btn-primary">Submit</button>
        <br>
      </form>


    </div>
